# platform :ios, '9.0'

target 'Gists' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Gists
  pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :branch => 'alamofire5'
